            "args": [
                "clients/sdk/src/generated/nft-operations",
                {
                    "dependencyMap": {
                        "solanaAccounts": "gill",
                        "solanaAddresses": "gill",
                        "solanaCodecsCore": "gill",
                        "solanaCodecsDataStructures": "gill",
                        "solanaCodecsNumbers": "gill",
                        "solanaCodecsStrings": "gill",
                        "solanaErrors": "gill",
                        "solanaInstructions": "gill",
                        "solanaOptions": "gill",
                        "solanaPrograms": "gill",
                        "solanaRpcTypes": "gill",
                        "solanaSigners": "gill"
                    }
                }
            ]
        },
        "rust": {
            "from": "@codama/renderers-rust",
            "args": [
                "clients/rust/src/generated/nft-operations",
                {
                    "crateFolder": "clients/rust",
                    "formatCode": true
                }
            ]
        }
    }
